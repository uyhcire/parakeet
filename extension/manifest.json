{
  "name": "Parakeet",
  "description": "A sidekick to help write code in notebooks",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.colab.research.google.com/*"],
      "js": ["main.tsx"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
